{
	"name": "terrace.js",
	"version": "0.4.4",
	"private": false,
	"scripts": {
		"bin-dev": "nodemon --ignore bin/index.generated.js --ignore bin/_index.generated.js --exec 'yarn build-bin'",
		"write-npmignore": "cat .gitignore > .npmignore && cat .extra.npmignore >> .npmignore ",
		"write-gitignore-extra": "cat .header.gitignore > .extra.gitignore && cat .gitignore >> .extra.gitignore",
		"prepare": "yarn write-npmignore",
		"build-bin": "bash ./bin/build-bin.sh",
		"preupdate": "yarn prepare && yarn build-bin && yarn write-gitignore-extra",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build-js": "react-scripts build",
		"build": "npm-run-all build-css build-js"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"axios-mock-adapter": "^1.15.0",
		"classnames": "^2.2.6",
		"custom-react-scripts": "^0.2.2",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"http-status-codes": "^1.3.0",
		"i18next": "^11.5.0",
		"i18next-browser-languagedetector": "^2.2.3",
		"js-cookie": "^2.2.0",
		"ls": "^0.2.1",
		"material-design-icons": "^3.0.1",
		"mock-local-storage": "^1.0.5",
		"query-string": "5",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-i18next": "^7.10.1",
		"react-redux": "^5.0.7",
		"react-resize-detector": "^3.0.1",
		"react-router-dom": "^4.3.1",
		"react-select": "^2.0.0-beta.7",
		"react-spinkit": "^3.0.0",
		"react-svg-loader": "^2.1.0",
		"redux": "^4.0.0",
		"redux-devtools-extension": "^2.13.5",
		"redux-form": "^7.4.2",
		"redux-immutable-state-invariant": "^2.1.0",
		"redux-saga": "^0.16.0",
		"storage.io": "^1.2.0",
		"voca": "^1.4.0"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.6",
		"cli-progress": "^2.1.0",
		"colors": "^1.3.1",
		"emoji-regex": "^7.0.0",
		"eslint": "^4.1.1",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-flowtype": "^2.50.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.11.1",
		"file-system": "^2.2.2",
		"flow-bin": "^0.75.0",
		"ncp": "^2.0.0",
		"node-sass-chokidar": "^1.3.0",
		"npm-run-all": "^4.1.3",
		"pluralize": "^7.0.0",
		"react-test-renderer": "^16.4.2",
		"readline-sync": "^1.4.9",
		"strip-json-comments": "^2.0.1",
		"yargs": "^12.0.0"
	},
	"description": "A `create-react-app` based frontend starter project, using `custom-react-scripts`.",
	"main": "build/index.html",
	"repository": "https://github.com/Shuttl-Tech/terrace.js",
	"author": "jayantbh <bhawal.jayant@gmail.com>",
	"license": "MIT",
	"bin": {
		"terrace": "bin/index.generated.js"
	}
}
