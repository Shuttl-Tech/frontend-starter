{
  "name": "terrace.js",
  "version": "0.7.4",
  "private": false,
  "scripts": {
    "bin-dev": "nodemon --ignore bin/index.generated.js --ignore bin/_index.generated.js --exec 'yarn build-bin'",
    "write-npmignore": "cat .gitignore > .npmignore && cat .extra.npmignore >> .npmignore ",
    "write-gitignore-extra": "cat .header.gitignore > .extra.gitignore && cat .gitignore >> .extra.gitignore",
    "prepare": "yarn write-npmignore",
    "build-bin": "bash ./bin/build-bin.sh",
    "preupdate": "yarn prepare && yarn build-bin && yarn write-gitignore-extra",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "original-start": "react-scripts start",
    "start": "yarn bin-dev",
    "build": "react-scripts build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "i18next": "^11.5.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "js-cookie": "^2.2.0",
    "ls": "^0.2.1",
    "mock-local-storage": "^1.0.5",
    "node-sass": "^4.10.0",
    "query-string": "5",
    "ramda": "^0.26.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-i18next": "^7.10.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "^0.16.0",
    "storage.io": "^1.2.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/i18next": "^12.1.0",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/jest": "^23.3.10",
    "@types/js-cookie": "^2.2.0",
    "@types/node": "^10.12.18",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/redux-immutable-state-invariant": "^2.1.0",
    "babel-eslint": "9.0.0",
    "cli-progress": "^2.1.0",
    "colors": "^1.3.1",
    "emoji-regex": "^7.0.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-system": "^2.2.2",
    "pluralize": "^7.0.0",
    "react-test-renderer": "^16.4.2",
    "readline-sync": "^1.4.9",
    "recursive-copy": "^2.0.9",
    "through2": "^3.0.0",
    "typescript": "^3.2.2",
    "yargs": "^12.0.0"
  },
  "description": "A `create-react-app` based frontend starter project, using `react-scripts` 2+.",
  "main": "build/index.html",
  "repository": "https://github.com/Shuttl-Tech/terrace.js",
  "author": "jayantbh <bhawal.jayant@gmail.com>",
  "license": "MIT",
  "bin": {
    "terrace": "bin/index.generated.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
